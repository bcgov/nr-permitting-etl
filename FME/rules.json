    "logic": {
      "and": [
        {
          "attr": "APPLICATION_RECEIVED_DATE IS NOT NUL",
          "op": "regex",
          "value": ".*"
        },
        {
          "attr": "APPLICATION_ACCEPTED_DATE",
          "op": "not_null"
        },
        {
          "attr": "FCBC_COMPLETED_DATE",
          "op": "not_null"
        },
        {
          "attr": "ON_HOLD_START_DATE",
          "op": ">=",
          "other_attr": "APPLICATION_ACCEPTED_DATE"
        }
      ]
    }
  },
