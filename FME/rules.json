  },
  "Withdrawn_Initial_submission_review": {
    "start_date": {
      "attr": "ADJUDICATION_DATE",
      "op": "not_null"
    },
    "logic": {
      "and": [
        {
          "attr": "ADJUDICATION_DATE",
          "op": "not_null"
        },
        {
          "attr": "ACCEPTED_DATE",
          "op": "null"
        },
        {
          "attr": "AUTHORIZATION_STATUS_CODE",
          "op": "=",
          "value": "2"
        },
        {
          "or": [
            {
              "attr": "ATHN_CLOSE_REASON_CODE",
              "op": "=",
              "value": "3"
            },
            {
              "attr": "ATHN_CLOSE_REASON_CODE",
              "op": "=",
              "value": "81"
            }
          ]
        }
      ]
    }
  },
